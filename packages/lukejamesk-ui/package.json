    "clsx": "^1.0.4",
    "global": "^4.4.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-focus-lock": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
