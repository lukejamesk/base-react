    "directory": "packages/lukejamesk-ui"
  },
  "scripts": {
    "build": "npm run ts:typecheck && npm run build:clear && npm run build:cjs && npm run build:copy-files && npm run build:types",
    "build:cjs": "NODE_ENV=production BABEL_ENV=production && ../../node_modules/.bin/babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .ts,.tsx,.js,.jsx --source-maps true",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:clear": "npx rimraf build",
    "ts:typecheck": "tsc --noEmit --jsx react",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@lukejamesk/icons": "^0.0.4-alpha.3",
