  "private": false,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukejamesk/lukejamesk-ui.git",
    "directory": "packages/lukejamesk-ui-styles"
  },
  "scripts": {
    "build": "yarn build:pre && yarn build:cjs && yarn build:copy-files",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build  --ignore \"**/*.test.js\"",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:pre": "rimraf build",
    "release": "yarn build && npm publish ./build --tag latest --registry http://localhost:4873"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-jss": "10.0.0-alpha.24",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "enzyme": "^3.10.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
