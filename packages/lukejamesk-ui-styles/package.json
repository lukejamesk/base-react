{
  "name": "@lukejamesk/styles",
  "version": "0.0.4-alpha.3",
  "license": "MIT",
  "author": "Luke King",
  "private": false,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukejamesk/lukejamesk-ui.git",
    "directory": "packages/lukejamesk-ui-styles"
  },
  "scripts": {
    "build": "npm run build:clear && npm run build:cjs && npm run copy:styles && npm run build:copy-files",
    "build:cjs": "NODE_ENV=production BABEL_ENV=production ../../node_modules/.bin/babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .ts,.tsx,.js,.jsx --source-maps true",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:clear": "npx rimraf build",
    "copy:styles": "cp -a src/css/ build/css/",
    "build:tailwind": "postcss src/css/ui.css -o build/css/dist.css --config"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "autoprefix": "^1.0.1",
    "autoprefixer": "^9.7.6",
    "postcss-cli": "^7.1.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "tailwindcss": "^1.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "gitHead": "541de791aacfdd67ab405fc4ae658636205e90a1"
}
